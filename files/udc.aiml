<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
  <category>
    <pattern>HI</pattern>
    <template>Welcome to the computing career advisor. I can help you learn more about a career or I can help you decide based on your strengths and preferences. What do you need help with?
        <button>
            <text>Learn more</text>
            <postback>Learn more</postback>
        </button>
        <button>
            <text>Help me choose</text>
            <postback>helpme</postback>
        </button>
    </template>
  </category>
 
